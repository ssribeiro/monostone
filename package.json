    "clean": "rimraf ./dist",
    "generate-docs": "./node_modules/.bin/typedoc --readme readme.md --theme minimal --out docs/typedocs/ src/"
  },
  "repository": {
    "type": "git",
    "url": "/home/ss/gd/repos/monostone"
  },
  "keywords": [
    "monolith",
    "framework",
