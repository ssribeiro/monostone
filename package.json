    "@types/module-alias": "^2.0.0",
    "@types/mongodb": "^3.1.19",
    "@types/node": "^10.12.21",
    "@types/node-fetch": "^2.1.4",
    "@types/sanitize-html": "^1.18.2",
    "@types/supertest": "^2.0.7",
    "@types/uuid": "^3.4.4",
