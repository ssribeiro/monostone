{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "outDir": "dist",
    "lib": ["es2016","dom"],
    "sourceMap": true,
    "strict": true,
    "moduleResolution": "node",
    "typeRoots": ["node_module/@types","./typings","./types"],
    "types": [
      "node",
      "jasmine",
      "jasmine-expect"
    ]
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
